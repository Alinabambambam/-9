internal class Program
{
    static void Main(string[] args)
    {
        person PersonObject = new person();
        PersonObject.Name = "Masha";
        PersonObject.Age = -5;
        PersonObject.SayHello();

    }
}

    internal class person
    {
        private int age;
        private string name;

        public int Age
        {
            get { return age; }
            set
            {
                if (age <= 0 || age >= 120)
                {
                    Console.WriteLine("ошибка");
                }
                else
                { age = value; }
            }
        }

        public string Name
        {
            get { return name; }
            set
            {
                if (string.IsNullOrEmpty(value))
                {
                    Console.WriteLine("Строка пустая или null");
                }
                else
                {
                    name = value;
                }
            }
        }
        public void SayHello()
        { Console.WriteLine($"Привет, я {name}, мне {age} лет"); }
    }
}
